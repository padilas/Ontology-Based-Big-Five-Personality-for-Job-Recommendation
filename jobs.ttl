#################################################################
# PROPERTY CHAINS (lanjutan, setelah definisi properties di Part 1)
#################################################################

# Candidate hasFacet → facetOfTrait → hasPersonalityTrait
:hasPersonalityTrait
    owl:propertyChainAxiom ( :hasFacet :facetOfTrait ) .

# Experience → WordExpression → Facet/Trait → Candidate
:hasFacet
    owl:propertyChainAxiom ( :hasExperience :mentionsWord :representsFacet ) .

:hasPersonalityTrait
    owl:propertyChainAxiom ( :hasExperience :mentionsWord :representsTrait ) .

# JobField hierarchy propagation
:inJobField
    owl:propertyChainAxiom ( :inJobField :hasSubField ) .

#################################################################
# QUALIFIED CARDINALITY (Q) – Global Constraints
#################################################################

# Kandidat minimal punya 1 trait dan 1 soft skill
:Candidate rdfs:subClassOf
    [ a owl:Restriction ;
      owl:onProperty :hasPersonalityTrait ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass :PersonalityTrait
    ] ,
    [ a owl:Restriction ;
      owl:onProperty :hasSoftSkill ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass :SoftSkill
    ] .

# JobOccupation minimal punya 1 requiresHardSkill
:JobOccupation rdfs:subClassOf
    [ a owl:Restriction ;
      owl:onProperty :requiresHardSkill ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass :HardSkill
    ] .

