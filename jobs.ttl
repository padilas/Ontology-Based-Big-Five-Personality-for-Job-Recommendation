            [ owl:onProperty :hasHardSkill ; owl:hasValue :DataAnalysisSkill ]
            [ owl:onProperty :hasHardSkill ; owl:hasValue :MachineLearningSkill ]
            [ owl:onProperty :hasHardSkill ; owl:hasValue :StatisticsSkill ]
            [ owl:onProperty :hasSoftSkill ; owl:hasValue :AnalyticalThinkingSkill ]
        )
    ] .

:MediumMatch_DataScientist a owl:Class , :MatchCategory ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (
            :Candidate
            [ owl:onProperty :hasHardSkill ; owl:hasValue :DataAnalysisSkill ]
