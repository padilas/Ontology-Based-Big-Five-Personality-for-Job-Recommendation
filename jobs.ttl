#################################################################
# NOMINALS (O) – Skill cluster untuk Software Engineer
#################################################################

:RequiredSESkills owl:oneOf ( :ProgrammingSkill :JavaSkill :PythonSkill :TestingQASkill :GitSkill ) .

#################################################################
# MATCH CATEGORY CLASSES (High / Medium / Low) – Job-specific
#################################################################

# HighMatch_SoftwareEngineer ≈ punya hampir semua skill penting + trait C & O
:HighMatch_SoftwareEngineer a owl:Class , :MatchCategory ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (
            :Candidate
            [ owl:onProperty :hasPersonalityTrait ; owl:hasValue :ConscientiousnessTrait ]
            [ owl:onProperty :hasPersonalityTrait ; owl:hasValue :OpennessTrait ]
            [ owl:onProperty :hasHardSkill ; owl:hasValue :ProgrammingSkill ]
            [ owl:onProperty :hasHardSkill ; owl:hasValue :JavaSkill ]
            [ owl:onProperty :hasHardSkill ; owl:hasValue :TestingQASkill ]
            [ owl:onProperty :hasHardSkill ; owl:hasValue :GitSkill ]
            [ owl:onProperty :hasSoftSkill ; owl:hasValue :ProblemSolvingSkill ]
            [ owl:onProperty :hasSoftSkill ; owl:hasValue :TeamworkSkill ]
        )
    ] .

:MediumMatch_SoftwareEngineer a owl:Class , :MatchCategory ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (
            :Candidate
            [ owl:onProperty :hasHardSkill ; owl:hasValue :ProgrammingSkill ]
            [ owl:onProperty :hasSoftSkill ; owl:hasValue :ProblemSolvingSkill ]
        )
    ] .

:LowMatch_SoftwareEngineer a owl:Class , :MatchCategory ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (
            :Candidate
            [ owl:onProperty :hasHardSkill ; owl:hasValue :ProgrammingSkill ]
        )
    ] .

# HighMatch_DataScientist – punya DataAnalysis + ML + Statistics + AnalyticalThinking + Openness
:HighMatch_DataScientist a owl:Class , :MatchCategory ;
    owl:equivalentClass [
