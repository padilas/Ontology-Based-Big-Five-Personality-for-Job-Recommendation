#################################################################
# ONTOLOGY METADATA
#################################################################

<http://www.semanticweb.org/asyifafadhilah/ontologies/2025/10/recruitment-ontology> a owl:Ontology ;
    rdfs:label "Recruitment Ontology – Full SROIQ(D)" ;
    owl:versionInfo "3.0" ;
    rdfs:comment "Ontology integrates personality test (Question→Facet→Trait), 100+ skills, 50+ jobs, weighted matching, job suggestions, and SROIQ(D) expressivity." .

#################################################################
# CORE CLASSES
#################################################################

:Person a owl:Class .
:Candidate a owl:Class ; rdfs:subClassOf :Person .
:Recruiter a owl:Class ; rdfs:subClassOf :Person .

:Application a owl:Class .
:MatchCategory a owl:Class .
:SuggestionClass a owl:Class .

:Skill a owl:Class .
:SoftSkill a owl:Class ; rdfs:subClassOf :Skill .
:HardSkill a owl:Class ; rdfs:subClassOf :Skill .

:Experience a owl:Class .
:JobField a owl:Class .
:JobOccupation a owl:Class .

:PersonalityTrait a owl:Class .
:PersonalityFacet a owl:Class .
:PersonalityQuestion a owl:Class .
:WordExpression a owl:Class .

#################################################################
# DISJOINTNESS
#################################################################

:Person owl:disjointWith :Skill, :Experience, :JobField, :JobOccupation, :Application, :MatchCategory .
:Skill owl:disjointWith :Experience, :JobField, :JobOccupation, :PersonalityTrait, :PersonalityFacet .
:JobField owl:disjointWith :JobOccupation .
:PersonalityTrait owl:disjointWith :PersonalityFacet .

#################################################################
# OBJECT PROPERTIES
#################################################################

:hasPersonalityTrait a owl:ObjectProperty .
:hasFacet a owl:ObjectProperty .
:facetOfTrait a owl:ObjectProperty .
:hasFacetDefinition a owl:ObjectProperty .

:hasSkill a owl:ObjectProperty .
:hasSoftSkill a owl:ObjectProperty .
:hasHardSkill a owl:ObjectProperty .

:requiresSkill a owl:ObjectProperty .
:requiresSoftSkill a owl:ObjectProperty .
:requiresHardSkill a owl:ObjectProperty .

:hasExperience a owl:ObjectProperty .
:experienceInField a owl:ObjectProperty .

:mentionsWord a owl:ObjectProperty .
:representsFacet a owl:ObjectProperty .
:representsTrait a owl:ObjectProperty .

:inJobField a owl:ObjectProperty .
:hasSubField a owl:ObjectProperty .

:hasApplication a owl:ObjectProperty .
:hasApplicant a owl:ObjectProperty .
:forJob a owl:ObjectProperty .
:hasMatchCategory a owl:ObjectProperty .

:answeredBy a owl:ObjectProperty .
:measuresFacet a owl:ObjectProperty .

#################################################################
# (S) TRANSITIVE ROLES
#################################################################

:hasSubField a owl:TransitiveProperty .

# SROIQ requirement: at least one extra example
:oversees a owl:ObjectProperty , owl:TransitiveProperty .
:supervises a owl:ObjectProperty .
:manages a owl:ObjectProperty .

:manages rdfs:subPropertyOf :supervises .
:supervises rdfs:subPropertyOf :oversees .

#################################################################
# (R) ROLE HIERARCHY + ROLE INCLUSION
#################################################################

:hasHardSkill rdfs:subPropertyOf :hasSkill .
:hasSoftSkill rdfs:subPropertyOf :hasSkill .
:requiresHardSkill rdfs:subPropertyOf :requiresSkill .
:requiresSoftSkill rdfs:subPropertyOf :requiresSkill .

# Complex role inclusion → MAKES R ACTIVE → SROIQ(D)
:hasSkill
    owl:propertyChainAxiom ( :hasHardSkill :hasSoftSkill ) .

#################################################################
# INVERSE ROLES (I)
#################################################################

:hasApplication owl:inverseOf :hasApplicant .
:hasFacetDefinition owl:inverseOf :facetOfTrait .

#################################################################
# DATA PROPERTIES (D)
#################################################################

:yearsOfExperience a owl:DatatypeProperty ; rdfs:range xsd:int .
:hasTitle a owl:DatatypeProperty ; rdfs:range xsd:string .
:hasDescription a owl:DatatypeProperty ; rdfs:range xsd:string .
:questionScore a owl:DatatypeProperty ; rdfs:range xsd:int .

